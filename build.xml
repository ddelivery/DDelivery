<!-- Build for weboshot. Pretty much the only purpose is to deploy the apache 
    htdocs as defined in build.properties -->
<project name="weboshot" default="deploy" basedir=".">
 
 
 <target name="deploy">
 
     <delete dir="/var/www/ddelivery"></delete>
 	
 	 <mkdir dir="/var/www/ddelivery"></mkdir>
 
     <copy todir="/var/www/ddelivery">
 
         <fileset dir="public_html/"></fileset>
 
     </copy>
 
 </target>
 
</project>